{
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@typescript-eslint/parser"
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "vue/no-multiple-template-root": "off",
    "no-var": "error"
  },
  "extends": [
    "plugin:vue/vue3-recommended"
  ]
}